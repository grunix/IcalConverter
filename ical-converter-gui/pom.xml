<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
      <artifactId>ical-converter</artifactId>
      <groupId>org.gruner.ical</groupId>
      <version>1.0-SNAPSHOT</version>
  </parent>
  <artifactId>ical-converter-gui</artifactId>
  <packaging>jar</packaging>

  <name>ical-converter-gui</name>
  <url>http://maven.apache.org</url>
  <repositories>
    <repository>
      <url>http://download.java.net/maven/2/</url>
      <id>beans-binding</id>
      <layout>default</layout>
      <name>Repository for library Library[beans-binding]</name>
    </repository>
  </repositories>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <eclipseLink>TRUE</eclipseLink>
  </properties>
  <build>
      <plugins>
         <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-compiler-plugin</artifactId>
            <version>2.3.2</version>
            <configuration>
               <source>1.6</source>
               <target>1.6</target>
            </configuration>
         </plugin>
      </plugins>
      
  </build>
 
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.gruner.ical</groupId>
      <artifactId>ical-converter-service</artifactId>
      <version>${project.version}</version>
    </dependency>
    <dependency>
      <groupId>org.jdesktop</groupId>
      <artifactId>swing-worker</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jdesktop</groupId>
      <artifactId>appframework</artifactId>
    </dependency>
    <dependency>
      <groupId>org.jdesktop</groupId>
      <artifactId>beansbinding</artifactId>
    </dependency>
    <dependency>
              <groupId>net.sf.datavision</groupId>
      <artifactId>DataVision</artifactId>

        </dependency>
    <dependency>
      <groupId>com.toedter</groupId>
      <artifactId>jcalendar</artifactId>
    </dependency>
  </dependencies>
 
</project>
